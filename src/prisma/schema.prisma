generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Company {
  id    String @id @default(uuid())
  nome  String
}

model CategoryMapping {
  id      String @id @default(uuid())
  origem  String
  destino String
}

model EscalaMapping {
  id      String  @id @default(uuid())
  empresa String
  origem  String
  destino String
  ativa   Boolean @default(true)
}

model VinculoMapping {
  id    String  @id @default(uuid())
  sigla String  @unique
  nome  String
  ativa Boolean @default(true)
}